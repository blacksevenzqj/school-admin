<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="school.management.business.visa.dao.mybatis.BaseInformationDao">

    <select id="getById" resultType="school.management.business.visa.entity.BaseInformation">
        select * from qz_base_information where base_information_id = #{id}
    </select>

    <select id="getBaseInformationList" resultType="school.management.business.visa.entity.BaseInformation">
        select * from qz_base_information
    </select>

    <insert id="insert" parameterType="school.management.business.visa.entity.BaseInformation">
    insert into qz_base_information
    (
    `name`,
    `description`
    )
    VALUES (
      #{name},
      #{description}
    )
    </insert>

</mapper>