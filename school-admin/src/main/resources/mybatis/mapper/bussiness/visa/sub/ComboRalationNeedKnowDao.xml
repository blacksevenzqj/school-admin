<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="school.management.business.visa.dao.mybatis.ComboRalationNeedKnowDao">

    <insert id="insert" parameterType="school.management.business.visa.entity.ComboRalationNeedKnow">
    insert into qz_combo_ralation_need_know
    (
    `visa_id`,
    `combo_id`,
    `need_know_id`,
    `create_time`,
    `del_flag`
    )
    VALUES (
      #{visaId},
      #{comboId},
      #{needKnowId},
      #{createTime},
      #{delFlag}
    )
    </insert>

    <insert id="insertBatch" parameterType="java.util.List">
        insert into qz_combo_ralation_need_know
        (
        `visa_id`,
        `combo_id`,
        `need_know_id`,
        `create_time`,
        `del_flag`
        )
        values
        <foreach collection="list" item="item" index="index" separator=",">
            (
            #{item.visaId,jdbcType=INTEGER}, #{item.comboId,jdbcType=INTEGER}, #{item.needKnowId,jdbcType=INTEGER},
            #{item.createTime,jdbcType=DATE}, #{item.delFlag,jdbcType=VARCHAR}
            )
        </foreach>
    </insert>


    <update id="update" parameterType="school.management.business.visa.entity.ComboRalationNeedKnow">
        UPDATE qz_combo_ralation_need_know
        <trim prefix="set" suffixOverrides="," suffix=" where id = #{id} ">
            <if test="visaId != null"> visa_id = #{visaId} , </if>
            <if test="comboId != null"> combo_id = #{comboId} , </if>
            <if test="needKnowId != null"> need_know_id = #{needKnowId} , </if>
            <if test="createTime != null" > create_time = #{createTime} , </if>
            <if test="delFlag != null" > del_flag = #{delFlag} , </if>
        </trim>
    </update>

    <delete id="cneedKnowDelBatchBycomboIds">
        delete from qz_combo_ralation_need_know where combo_id in
        <foreach collection="ids" item="item" index="index" open="(" separator="," close=")">
            #{item}
        </foreach>
    </delete>



</mapper>